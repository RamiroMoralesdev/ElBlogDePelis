<script>

  const movies = '/public/favicon.png | Aquaman | 2023 | https://drive.google.com/file/d/1fU5rCO8BghqfvRP1uDk6u-0LjsVIpx4i/preview';
  const markdownString = movies;
  
  const lines = markdownString.trim().split('\n');
  const dataArray = lines.map(line => {
    const [coverImage, titulo, fecha, video] = line.split('|');
    return { coverImage, titulo, fecha: parseInt(fecha), video };
  });
  
  console.log(dataArray);
  
  const container = document.getElementById('container');
  
  dataArray.forEach(movie => {
    const div = document.createElement('div');
    const img = document.createElement('img');
    const frame = document.getElementById('frame')
  
    img.src = movie.coverImage;
    img.alt = "Movie Cover";
    img.width = 100;
  
    div.appendChild(img);
  
    frame.src = movie.video;
  
    div.innerHTML += `
      <p>Nombre: ${movie.titulo}</p>
      <p>Lanzamiento: ${movie.fecha}</p>
    `;
  
    container.appendChild(div);
  
  });
  
  
  
  </script>
  
  <div id="container" class="text-white font-serif text-3xl flex flex-column-reverse">
    <iframe id="frame" src=""  allowfullscreen width="500" height="300"></iframe>
  </div>
  
  